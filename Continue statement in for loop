//Demonstration of continue statement in for loop
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication18
{
classProgram
    {
staticvoid Main(string[] args)
        {
for(int x = 1; x <= 18; x++) 
            { 
if (x == 3) 
             { 
continue; 
              } 
Console.WriteLine(x);
            }
Console.ReadLine();

        }
    }
}
