//for loop even no
using System;

namespace ConsoleApplication14
{
classProgram
    {
staticvoid Main(string[] args)
        {
for (int i = 0; i <= 10; i++)
            {
if (i%2==0)
                {
Console.Write(i);
                }
            }
Console.ReadLine();
        }
    }
}
//program odd no using loop
using System;

namespace ConsoleApplication14
{
classProgram
    {
staticvoid Main(string[] args)
        {
for (int i = 0; i <= 10; i++)
            {
if (i%2!=0)
                {
Console.Write(i);
                }
            }
Console.ReadLine();
        }
    }
}
