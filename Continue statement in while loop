//demonstration of continue statement in while loop.
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication19
{
classProgram
    {
staticvoid Main(string[] args)
        {
int i=1;
while(i<=21)
           {
	        i++;
if(i==5)
           {
	continue;
           }
Console.WriteLine(i);
           }
Console.ReadLine();
        }
    }
}
